    <DialogContent>
        <MudText>@Resource.Do_you_want_to_remove_this_record</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">@Resource.Cancel</MudButton>
        <MudButton Color="@Color.Warning" Variant="Variant.Filled" OnClick="Submit">@Resource.Delete</MudButton>
    </DialogActions>
</MudDialog>
