<MudDialog>
    <DialogContent>
        <MudText>@Resource.Are_you_sure_you_want_to_save_this_record</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">@Resource.Cancel</MudButton>
        <MudButton Color="@Color.Primary" Variant="Variant.Filled" OnClick="Submit">@Resource.Save</MudButton>
    </DialogActions>
</MudDialog>
